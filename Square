package arraylist;

public class Square {
	
	private double side;
	
	public double s;
	public double getArea;
	
	public Square() {
		side=10.05;
		s=5.5;
		getArea = side * s;
	}//end empty argument constructor

	public double getS() {
		return s;
	}//end getS

	public void setS(double s) {
		this.s = s;
	}//end setS

	public double getGetArea() {
		return getArea;
	}//end getGetArea

	public void setGetArea(double getArea) {
		this.getArea = getArea;
	}//end setGetArea

	@Override
	public String toString() {
		return "Square [side=" + side + ", s=" + s + ", getArea=" + getArea + "]";
	}//end toString
	
}//end class
